{
  "$schema": "https://json.schemastore.org/package",
  "name": "terminal",
  "description": "Web-based developer tools with a dev-friendly terminal interface.",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22.11",
    "npm": ">=10.9"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liam-hales/terminal.git"
  },
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "build:analyse": "ANALYSE_BUNDLE=true next build",
    "build:icons": "ts-node ./scripts/buildIcons.script.ts",
    "lint": "next lint",
    "deps": "ncu --packageManager yarn",
    "cleanup": "rm -rf .next/"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.1.1",
    "camelcase-keys": "^9.1.3",
    "change-case": "^5.4.4",
    "dayjs": "^1.11.18",
    "dedent": "^1.7.0",
    "fast-fuzzy": "^1.12.0",
    "fflate": "^0.8.2",
    "nanoid": "^5.1.5",
    "next": "^15.5.2",
    "postcss": "^8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwindcss": "^4.1.12",
    "typewriter-effect": "^2.22.0",
    "uuid": "^11.1.0",
    "whoiser": "^1.18.0",
    "yargs-parser": "^22.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/xml2js": "^0.4.14",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "npm-check-updates": "^18.0.3",
    "sharp": "^0.34.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "xml2js": "^0.6.2"
  }
}
