{
  "name": "terminal",
  "description": "Web-based developer tools with a dev-friendly terminal interface.",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.10",
    "npm": ">=10.2"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liam-hales/terminal.git"
  },
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
