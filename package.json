{
  "$schema": "https://json.schemastore.org/package",
  "name": "terminal",
  "description": "Web-based developer tools with a dev-friendly terminal interface.",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22.11",
    "npm": ">=10.9"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liam-hales/terminal.git"
  },
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "start:dev": "next dev",
