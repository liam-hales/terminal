    "camelcase-keys": "^9.1.3",
    "change-case": "^5.4.4",
    "cli-spinners": "^3.2.0",
    "dayjs": "^1.11.13",
    "fast-fuzzy": "^1.12.0",
    "nanoid": "^5.0.8",
    "next": "15.0.3",
    "postcss": "8.4.49",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.15",
    "typewriter-effect": "^2.21.0",
    "uuid": "^11.0.3",
    "whoiser": "^1.18.0",
    "yargs-parser": "^21.1.1",
    "zod": "^3.23.8"
