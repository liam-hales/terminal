  "dependencies": {
    "autoprefixer": "10.4.20",
    "camelcase-keys": "^9.1.3",
    "change-case": "^5.4.4",
    "cli-spinners": "^3.2.0",
    "dayjs": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.7",
    "next": "14.2.11",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.12",
    "typewriter-effect": "^2.21.0",
    "whoiser": "^1.18.0",
    "yargs-parser": "^21.1.1",
    "zod": "^3.23.8"
