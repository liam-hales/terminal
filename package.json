    "nanoid": "^5.1.5",
    "next": "15.2.3",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwindcss": "^4.0.15",
    "typewriter-effect": "^2.21.0",
    "uuid": "^11.1.0",
